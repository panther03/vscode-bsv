{
  "name": "bluespec",
  "displayName": "bluespec",
  "description": "Syntax highlighting for Bluespec SystemVerilog",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "bluespec",
        "aliases": [
          "Bluespec SystemVerilog",
          "bluespec"
        ],
        "extensions": [
          ".bsv"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "bluespec",
        "scopeName": "source.bsv",
        "path": "./syntaxes/bluespec.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  }
}
